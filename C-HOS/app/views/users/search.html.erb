<h1>ユーザ検索</h1>
<%= form_with url: search_users_path, method: :get, local: true do |f| %>
  <%= f.text_field :user_name %>
  <%= f.submit :検索 %>
<% end %>
<table>
  <thead>
    <tr>
      <th>名前</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.user_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>